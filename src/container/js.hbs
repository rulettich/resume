<header>
   <div>
      {{> title data="JavaScript" big=true}}
    </div>
    <div>
      <em>{{description}}</em>
    </div>
    <br>
     {{#with history}}
       <div>History: </div>
       <br>
       <div>{{year}}</div>
       <div>Founder: <b>{{founder}}</b></div>
     {{/with}}
     <div class="mt-3">
      <hr>
     </div>
</header>
<main>
   <section>
       {{> title data="Types" big=false}}
           
            <div class="ms-4 mt-3">{{types}}</div>
            </div>
         <div class="ms-2 mt-3">
        {{> title data="Syntax" big=false}}
        </div> 
           {{#with syntax}}
            
              <div class="ms-4 mt-3">Variables: <b>{{variables}}</b></div>
              
                <div class="ms-4 mt-3">Functions: <b>{{functions}}</b></div>
                
                  <div class="ms-4 mt-3">Conditionals: <b>{{conditionals}}</b></div>
                  
                   <div class="ms-4 mt-3">Loops: <b>{{loops}}</b></div>
                   
                     <div class="ms-4 mt-3">Classes: <b>{{classes}}</b></div>
                     
            {{/with}}
        <div class="ms-2 mt-3">
          {{> title data="Features" big=false}}
        </div>
           
            <div class="ms-4 mt-3">{{features}}</div>
          <div class="ms-2 mt-3">
         {{> title data="Libraries" big=false}}
         </div> 
          <ul>
            <li><b>{{libraries}}</b></li>
          </ul>
        <div class="ms-2 mt-3">
        {{> title data="Tools" big=false}}
        </div>
         <ul>
            <li>
              <div><b>{{tools}}</b></div>
              </li>
         </ul>
         {{> title data="Community" big=false}}
           {{#each community}}
             
              <div class="ms-4 mt-3"><b>{{name}}</b></div>
              <div class="ms-4 mt-3">({{type}})</div>
              
           {{/each}}
    </section>
</main>