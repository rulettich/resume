<header>
  <div>
      {{>title data="Toyota Camry" big=true}}
  </div>
    <div>Make: <em>{{make}}</em></div>
    <div>Model: <em>{{model}}</em></div>
    <div>Year: <em>{{year}}</em></div>
     <div>Color: <em>{{color}}</em></div>
  </header> 
<div class="mt-3">
   <hr/>
 </div>  
<main>
  {{#with features}}
   <section>
       {{>title data="Features" big=true}}
         <div class="ms-3 mt-3">
             <h1>Interior</h1>
        </div>
         
          {{#with interior}}
          
           {{#with seats}}
           <div><b>Seats: </b></div>
           <div class="ms-3 mt-3">
              <div>material: <em>{{material}}</em></div>
              <div>color: <em>{{color}}</em></div>
              <div>heated: <em>{{heated}}</em></div>
              <div>ventilated: <em>{{ventilated}}</em></div>
              </div>
           {{/with}}

             <div><b>Dashboard: </b></div>
               {{#with dashboard}}
               <div class="ms-3 mt-3">
               <div>Material: <em>{{material}}</em></div>
                 <div>Color: <em>{{color}}</em></div>
                 </div>
                   <div><b>Display: </b></div>
                   {{#with display}}
                   <div class="ms-3 mt-3">
                     <div>Type: <em>{{type}}</em></div>
                      <div>Size: <em>{{size}}</em></div>
                      <div>Resolution: <em>{{resolution}}</em></div>
                      <div>Touchscreen: <em>{{touchscreen}}</em></div>
                      </div>
                   {{/with}}

               {{/with}}
                
                   <div><b>Audio: </b></div>
                    {{#with audio}}
                     <div class="ms-3 mt-3">
                       <div>System: <em>{{system}}</em></div>
                       <div>Speakers: <em>{{speakers}}</em></div>
                       <div>Subwoofer: <em>{{subwoofer}}</em></div>
                       <div>Bluetooth: <em>{{bluetooth}}</em></div>
                       <div>USB: <em>{{USB}}</em></div>
                     </div>

                    {{/with}}
          {{/with}}
    </section>

   <section>  
               <div class="ms-3 mt-2">
                 <h1>Exterior</h1>
               </div>
              {{#with exterior}}
                      <div class="mt-2"><b>Weels:</b></div>
                  {{#with wheels}} 
                  
                    
                    <div class="ms-3 mt-2">
                    <div>Size: <em>{{size}}</em></div>
                    <div>Type: <em>{{type}}</em></div>
                    <div>Color: <em>{{color}}</em></div>
                    </div>
                  {{/with}}
                     <div><b>Headlights:</b></div>
                  {{#with headlights}}
                    <div class="ms-3 mt-2">
                      <div>Type: <em>{{type}}</em></div>
                      <div>Brightness: <em>{{brightness}}</em></div>
                      <div>Automatic: <em>{{automatic}}</em></div>
                    </div>
                  {{/with}}
                      <div><b>Sunroof:</b></div>
                 {{#with sunroof}} 
                   <div class="ms-3 mt-2">
                    <div>Type: <em>{{type}}</em></div>
                    <div>Size: <em>{{size}}</em></div>
                    <div>Automatic: <em>{{automatic}}</em></div>
                   </div>
                 {{/with}}
              {{/with}}
      </section> 
    <section>
        <div class="ms-3 mt-2">
         <h1>Safety</h1>
        </div>
              <div><b>Airbags:</b></div>
         {{#with safety}}
           {{#with airbags}}
          <div class="ms-3 mt-2">
           <div>Front: <em>{{front}}</em></div>
           <div>Side: <em>{{side}}</em></div>
           <div>Knee: <em>{{knee}}</em></div>
           <div>Rear: <em>{{rear}}</em></div>
           </div>
           {{/with}}
              <div><b>Assistance:</b></div>
              {{#with assistance}}
               <div class="ms-3 mt-2">
                <div>Blind spot monitoring: <em>{{ blind_spot_monitoring}}</em></div>
                <div>Rear cross traffic alert: <em>{{rear_cross_traffic_alert}}</em></div>
                <div>Lane departure warning: <em>{{lane_departure_warning}}</em></div>
                <div>Adaptive cruise control: <em>{{adaptive_cruise_control}}</em></div>
                <div>Collision warning: <em>{{collision_warning}}</em></div>
               </div>
             {{/with}}
          
        {{/with}}
    </section>
{{/with}}
<section>
        <div class="ms-3 mt-2">
          <h1>Engine</h1>
        </div>
    {{#with engine}}
        <div class="ms-3 mt-2">
          <div>Type: <em>{{type}}</em></div>
          <div>Displacement: <em>{{displacement}}</em></div>
          <div>Horsepower: <em>{{horsepower}}</em></div>
          <div>Torque: <em>{{torque}}</em></div>
        </div>
            <div><b>Transmission:</b></div> 
        {{#with transmission}}
         <div class="ms-3 mt-2">
            <div>Type: <em>{{type}}</em></div>
            <div>Gears: <em>{{gears}}</em></div>
          </div>
        {{/with}}
    {{/with}}
 </section> 
 <section>
   <div class="ms-3 mt-2">
      <h1>Fuel economy</h1>
        {{#with fuel_economy}}
          <div>City: <em>{{city}}</em></div>
            <div>Highway: <em>{{highway}}</em></div>
              <div>Combined: <em>{{combined}}</em></div>
        {{/with}}
   </div>
 </section> 
 <section>
   <div class="ms-3 mt-2">
     <h1>Price</h1>
       {{#with price}}
       
         <div>Base: <em>{{base}}</em></div>
           <div>Destination: <em>{{destination}}</em></div>
        
          {{#with options}}
          
            <div>Navigation: <em>{{navigation}}</em></div>
            <div>Moonroof: <em>{{moonroof}}</em></div>
             <div>Premium paint: <em>{{premium_paint}}</em></div>
          
          {{/with}}
          <br>
          <div><b>Total: <em>{{total}}</em></b></div>
       {{/with}}
   </div>
 </section>
</main>