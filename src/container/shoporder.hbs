<main>

  <div class='col-12'>
    <div class='row'>
      <div class='card'>
        {{#with navigation}}

          <div class='nav'>

            {{#each links}}
              <div class='nav-item'>
                <a
                  href='{{href}}'
                  class='nav-link'
                >{{text}}</a>
              </div>
            {{/each}}
          </div>
        {{/with}}
      </div>
    </div>
  </div>

  <div class='row'>
    <div class='col-8'>
      <header>
        <h1 class='p-4'>{{header}}</h1>
        {{#with contacts_data}}
          <h4 class='pt-4'>{{title}}</h4>

          <div class='row'>
            <div class='col-8'>
              <div class='row'>

                {{#each buttons}}

                  {{#if isPrimary}}

                    <div class='col-2 w-50 pt-3'>

                      <button
                        class='btn btn-warning btn-sm'
                      >{{text}}</button>
                    </div>

                  {{/if}}
                  {{#if isOutline}}

                    <div class='col-2 w-50 pt-3'>

                      <button
                        class='btn btn-outline-warning btn-sm'
                      >{{text}}</button>
                    </div>

                  {{/if}}

                {{/each}}
              </div>
            </div>
          </div>

          {{#with form}}

            {{#with input}}
              <div class='row g-2'>
                {{#with surname}}
                  <div class='col-4'>
                    <h6><label
                        class='form-label pt-4'
                      >{{label}}</label></h6>
                    <input
                      class='form-control'
                      value='{{placeholder}}'
                    />
                    <h6><div
                        class='form-text'
                      >{{caption}}</div></h6>
                  </div>
                {{/with}}
                {{#with name}}
                  <div class='col-4'>
                    <h6><label
                        class='form-label pt-4'
                      >{{label}}</label></h6>
                    <input
                      class='form-control'
                      value='{{placeholder}}'
                    />
                    <h6><div
                        class='form-text'
                      >{{caption}}</div></h6>
                  </div>
                {{/with}}
              </div>
              <div class='row g-2'>
                {{#with phone}}
                  <div class='col-4'>
                    <h6><label
                        class='form-label pt-4'
                      >{{label}}</label></h6>
                    <input
                      class='form-control'
                      value='{{placeholder}}'
                    />
                    <h6><div
                        class='form-text'
                      >{{caption}}</div></h6>
                  </div>
                {{/with}}
                {{#with email}}
                  <div class='col-4'>
                    <h6><label
                        class='form-label pt-4'
                      >{{label}}</label></h6>
                    <input
                      class='form-control'
                      value='{{placeholder}}'
                    />
                    <h6><div
                        class='form-text'
                      >{{caption}}</div></h6>
                  </div>
                {{/with}}
              </div>
            {{/with}}

            <div class='pt-5'>
              <hr />
            </div>
            {{#each switch}}
              <div class='form-check form-switch p-2 m-5'>
                <input
                  type='checkbox'
                  class='form-check-input'
                />
                {{#if isActive}}
                  <h6><div class='pt-2'>{{text}}</div></h6>

                {{else}}

                  <h6><div class='pt-2'>{{text}}</div></h6>
                {{/if}}
              </div>
            {{/each}}
          {{/with}}
        {{/with}}
      </header>
    </div>

    <div class='col-4 p-5'>
      <div class='row'>
        <div class='card m-2'>

          {{#each action_block}}
            <div class='nav p-1 m-1'>
              <div class='nav-item'>
                {{#if isSuccess}}
                  <button
                    class='btn btn-success btn-sm'
                  >{{text}}</button>
                {{/if}}
                {{#if isDanger}}
                  <button
                    class='btn btn-danger btn-sm'
                  >{{text}}</button>
                {{/if}}
                {{#if isInfo}}
                  <button
                    class='btn btn-info btn-sm'
                  >{{text}}</button>
                {{/if}}
              </div>
            </div>
          {{/each}}

        </div>
      </div>

      <div class='row'>
        <div class='card m-2'>
          <div class='card-body'>
            {{#with total}}
              <h5>{{title}}</h5>
              {{#each item_list}}
                <div>{{name}}: {{price}}</div>
              {{/each}}

              <hr />

              {{#each additional_info}}
                <div>{{text}}: {{value}}</div>
              {{/each}}

              <hr />
              {{#with total_price}}
                <div>{{text}}: {{value}}</div>
              {{/with}}
              <hr />
              {{#each button}}
                {{#if isPrimary}}
                  <button
                    class='btn btn-primary btn-sm'
                  >{{text}}</button>
                {{/if}}
                <br />
                {{#if isOutline}}
                  <button
                    class='btn btn-outline-primary btn-sm'
                  >{{text}}</button>
                {{/if}}
              {{/each}}
            {{/with}}
          </div>
        </div>
      </div>

    </div>

  </div>
  <div class='col-8'>
    <hr />
  </div>
  {{#with delivery}}
    <h4 class='p-2'>{{title}}</h4>

    <div class='col-8'>
      <div class='card'>
        <div class='card-body'>
          <div class='row'>

            {{#each select}}
              <div class='col-5'>

                <h6><label
                    class='form-label ms-4'
                  >{{label}}</label></h6>
                <select
                  class='form-select form-select-sm mx-4'
                >
                  <option selected>{{placeholder}}</option>
                  {{#each options}}
                    <option value='{{value}}'>
                      {{text}}</option>
                  {{/each}}
                </select>

              </div>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
    {{#each delivery_type}}
      <div class='col-4 pt-3'>
        <div class='form-check'>
          <label class='form-check-label'>{{type}}</label>
          <input
            name='delivery'
            type='radio'
            class='form-check-input'
          />
        </div>
      </div>
    {{/each}}
    <div class='col-8 pt-4'>
      <hr />
    </div>
    {{#with payment}}
      <h4 class='pst-3'>{{title}}</h4>
      {{#each list}}
        <div class='col-3'>
          <div class='form-check'>
            <input
              name='payment'
              type='radio'
              class='form-check-input'
            />
            <label class='form-check label'>{{type}}</label>
            <div
              class='form-label m-2'
            >{{description}}</div>
          </div>
        </div>
      {{/each}}
    {{/with}}
    <div class='col-8 pt-4'>
      <hr />
    </div>
    {{#with contact}}
      <h4 class='pst-3 pt-2'>{{title}}</h4>
      <div class='row g-2'>
        {{#with surname}}
          <div class='col-4'>
            <h6><label
                class='form-label pt-4'
              >{{label}}</label></h6>
            <input
              class='form-control'
              value='{{placeholder}}'
            />
            <h6><div
                class='form-text'
              >{{caption}}</div></h6>
          </div>
        {{/with}}
        {{#with name}}
          <div class='col-4'>
            <h6><label
                class='form-label pt-4'
              >{{label}}</label></h6>
            <input
              class='form-control'
              value='{{placeholder}}'
            />
            <h6><div
                class='form-text'
              >{{caption}}</div></h6>
          </div>
        {{/with}}
      </div>
      <div class='row g-2'>
        {{#with phone}}
          <div class='col-4'>
            <h6><label
                class='form-label pt-4'
              >{{label}}</label></h6>
            <input
              class='form-control'
              value='{{placeholder}}'
            />
            <h6><div
                class='form-text'
              >{{caption}}</div></h6>
          </div>
        {{/with}}
        {{#with email}}
          <div class='col-4'>
            <h6><label
                class='form-label pt-4'
              >{{label}}</label></h6>
            <input
              class='form-control'
              value='{{placeholder}}'
            />
            <h6><div
                class='form-text'
              >{{caption}}</div></h6>
          </div>
        {{/with}}
      </div>
    {{/with}}
    <div class='col-8 pt-4'>
      <hr />
    </div>
    <div class='col-8'>
      {{#with coment}}
        <label class='form-label'>{{label}}</label>
        <input
          class='form-control'
          value='{{placeholder}}'
        />
      {{/with}}
    </div>
  {{/with}}
  <div class='col-8 pt-4'>
    <hr />
  </div>
  {{#with coupon}}
    <h4 class='pst-3 pt-2'>{{title}}</h4>
    <div class='col-8'>
      {{#with select}}
        {{#with availabel_coupon}}
          <label class='form-label'>{{label}}</label>
          <select class='form-select form-select-lg'>
            <option selected>{{placeholder}}</option>
            {{#each options}}
              <option value='{{value}}'>{{text}}</option>
            {{/each}}
          </select>
        {{/with}}

        {{#with friend_coupon}}
          <label class='form-label pt-3'>{{label}}</label>
          <select class='form-select form-select-sm'>
            <option selected>{{placeholder}}</option>
            {{#each options}}
              <option value='{{value}}'>{{text}}</option>
            {{/each}}
          </select>
        {{/with}}
      {{/with}}
    </div>
    {{#with add}}
      <div class='col-8'>
        {{#with friend}}
          <label class='form-label pt-3'>{{label}}</label>
          <select class='form-select form-select-lg'>
            <option selected>{{placeholder}}</option>
          </select>
          <div class='form-text'>{{caption}}</div>
        {{/with}}
        {{#with colleague}}
          <label class='form-label pt-3'>{{label}}</label>
          <select class='form-select form-select-sm'>
            <option selected>{{placeholder}}</option>
          </select>
          <div class='form-text'>{{caption}}</div>
        {{/with}}
      </div>
    {{/with}}
  {{/with}}
  <div class='col-8 pt-4'>
    <hr />
  </div>
  {{#with quiz}}
    <h4 class='pst-3 pt-2'>{{title}}</h4>
    {{#with preference}}
      <div class='pt-2 pb-2'>{{title}}</div>
      {{#each options}}
        <div class='form-check'>
          <label class='form-check-label'>{{One}}</label>
          <input
            name='device'
            type='radio'
            class='form-check-input'
          />
        </div>
        <div class='form-check'>
          <label class='form-check-label'>{{Two}}</label>
          <input
            name='device'
            type='radio'
            class='form-check-input'
          />
        </div>
        <div class='form-check'>
          <label class='form-check-label'>{{Three}}</label>
          <input
            name='device'
            type='radio'
            class='form-check-input'
          />
        </div>
        <div class='form-check'>
          <label class='form-check-label'>{{Four}}</label>
          <input
            name='device'
            type='radio'
            class='form-check-input'
          />
        </div>
        <div class='form-check'>
          <label class='form-check-label'>{{Five}}</label>
          <input
            name='device'
            type='radio'
            class='form-check-input'
          />
        </div>
      {{/each}}
    {{/with}}
    <div class='col-8'>
      {{#with your_expirience}}
        <label class='form-label pt-3'>{{label}}</label>
        <input class='form-control' />
      {{/with}}
      {{#with purchase_frequency}}
        <label class='form-label pt-3'>{{label}}</label>
        <select class='form-select'>
          <option selected>{{placeholder}}</option>
          {{#each options}}
            <option value='{{value}}'>{{text}}</option>
          {{/each}}
        </select>
      {{/with}}
      {{#with factors}}
        <div class='pt-3'>{{title}}</div>
        {{#each options}}
          <div class='form-check pt-3'>
            <label class='form-check-label'>{{One}}</label>
            <input
              type='checkbox'
              class='form-check-input'
            />
          </div>
          <div class='form-check'>
            <label class='form-check-label'>{{Two}}</label>
            <input
              type='checkbox'
              class='form-check-input'
            />
          </div>
          <div class='form-check'>
            <label
              class='form-check-label'
            >{{Three}}</label>
            <input
              type='checkbox'
              class='form-check-input'
            />
          </div>
          <div class='form-check'>
            <label class='form-check-label'>{{Four}}</label>
            <input
              type='checkbox'
              class='form-check-input'
            />
          </div>
          <div class='form-check'>
            <label class='form-check-label'>{{Five}}</label>
            <input
              type='checkbox'
              class='form-check-input'
            />
          </div>
          <div class='form-check'>
            <label class='form-check-label'>{{Six}}</label>
            <input
              type='checkbox'
              class='form-check-input'
            />
          </div>
          <div class='form-check'>
            <label
              class='form-check-label'
            >{{Seven}}</label>
            <input
              type='checkbox'
              class='form-check-input'
            />
          </div>
        {{/each}}
      {{/with}}
    </div>
  {{/with}}
</main>
  <div class="col-12 pt-4">
     <hr>
  </div>
<footer class="pt-4">
   <div class='row g-5'>
      {{#each footer}}
        <div class="nav col-2">
          <div>
              {{#each this}}

                <a class="nav-link {{#if @last}} disabled {{/if}}" href='{{this.link}}'>{{this.text}}</a>

              {{/each}}
          </div>
        </div>
      {{/each}}
    </div>
  </div>
</footer>