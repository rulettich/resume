<div>
    {{#with main}}
  <main>
    <section>
        {{> title data="My educationss" big=true}}
            <ul>
                    {{#each educations}}
                        <li>
                            {{name}}
                            {{#unless isEnd}}
                                {{#if name isEnd=false}}
                                (ще в процесі)
                                {{/if}}
                            {{/unless}}    
                        </li>
                    {{/each}}
            </ul>
        </section>

        <section>
            {{> title data="My certificates" big=false}}
              <ul>

                 {{#each certificates}}
                   <li>
                      {{name}} 
                          
                           {{#if id includeZero=true}} ({{id}}) {{/if}}

                     
                   </li>

                 {{/each}}
              </ul>
        </section>
  </main>
   {{/with}}
</div>




